{
  "expo": {
    "name": "CRMOrbit",
    "slug": "crm-orbit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splash-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/icons/splash-dark.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.crmorbit.app",
      "icon": "./assets/icons/adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_MULTICAST_STATE",
        "NEARBY_WIFI_DEVICES",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "android.permission.READ_CONTACTS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_MULTICAST_STATE",
        "NEARBY_WIFI_DEVICES",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "android.permission.READ_CONTACTS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "@react-native-community/datetimepicker",
      "./android-linking-plugin.js",
      "expo-secure-store",
      "expo-camera",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow CRM Orbit to access your contacts."
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow CRM Orbit to access your calendar."
        }
      ],
      "expo-font",
      "expo-background-task"
    ],
    "extra": {
      "eas": {
        "projectId": "d02faca1-6ed6-4dfd-988e-dafcf731376a"
      }
    },
    "owner": "masked-kunsiquat",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d02faca1-6ed6-4dfd-988e-dafcf731376a"
    }
  }
}
